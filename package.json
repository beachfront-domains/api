  "dependencies": {
    "@inc/logger": "^2019.7.23",
    "archiver": "^3.1.1",
    "bcrypt": "^3.0.7",
    "express-graphql": "^0.9.0",
    "got": "^10.2.1",
    "graphql": "^14.5.8",
    "graphql-import": "^0.7.1",
    "jsonwebtoken": "^8.5.1",
    "mailgun-js": "^0.22.0",
    "mime-types": "^2.1.25",
    "nanoid": "^2.1.8",
    "redis": "^2.8.0",
    "restify": "^8.5.1",
    "restify-cors-middleware": "^1.1.1",
    "rethinkdb": "^2.4.2",
    "rethinkdb-ts": "^2.4.0-rc.16",
    "vne": "^2019.9.14"
  },
  "description": "Beachfront API",
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.7.4",
    "@babel/plugin-proposal-function-sent": "^7.7.4",
    "@babel/plugin-proposal-json-strings": "^7.7.4",
    "@babel/plugin-proposal-numeric-separator": "^7.7.4",
    "@babel/plugin-proposal-throw-expressions": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-syntax-import-meta": "^7.7.4",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-typescript": "^7.7.7",
    "@babel/register": "^7.7.7",
    "@inc/eslint-config": "^2019.10.22",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-import-graphql": "^2.7.0",
    "babel-plugin-inline-import": "^3.0.0",
    "chronver": "^2019.10.2-7.1",
    "colorette": "^1.1.0",
    "cwd": "^0.10.0",
    "dedent": "^0.7.0",
    "eslint": "^6.8.0",
    "husky": "^3.1.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "snazzy": "^8.0.0",
    "standardx": "^5.0.0",
    "ts-node": "^8.5.4",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4",
    "updates": "^9.3.3"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm run increment && git add -A :/"
    }
  },
  "main": "api/index.js",
  "name": "beachfront-api",
  "private": true,
  "scripts": {
    "build": "npm run clean;tsc",
    "clean": "rimraf api",
    "format": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "increment": "chronver --increment package",
    "start": "npm i;npm run build;BABEL_DISABLE_CACHE=1 NODE_ENV=production node ./api/src/index.js",
    "test": "run-s test:*",
    "test:dependencies": "updates --update ./",
    "test:lint": "standardx --verbose | snazzy",
    "test:typescript": "tsc",
    "watch": "npm run build;BABEL_DISABLE_CACHE=1 NODE_ENV=development nodemon"
  },
  "standardx": {
    "ignore": [
      "api"
    ]
  },
  "version": "2020.01.04"
}
