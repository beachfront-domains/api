    "hooks": {
      "pre-commit": "npm run build && npm run pretty && npm run increment && git add -A :/"
    }
  },
  "main": "dist/index.js",
  "name": "beachfront-api",
  "prettier": "@webb/config-prettier",
  "private": true,
  "scripts": {
    "build": "npm run cleanup && tsc",
    "cleanup": "del './dist'",
    "decrypt:staging": "env-decrypt staging",
    "encrypt:staging": "env-encrypt staging",
    "increment": "chronver --increment package",
    "pretty": "prettier --write 'dist/**/*.ts'",
    "start": "npm i;npm run build;NODE_ENV=production PORT=2413 ts-node -r tsconfig-paths/register src/index.ts",
    "test": "run-s test:*",
    "test:dependencies": "updates --update ./ --exclude 'husky'",
    "test:editorconfig": "editorconfig-checker",
    "test:lint": "tsc",
    "watch": "NODE_ENV=development nodemon",
    "--start:watch": "tsc-watch --onSuccess 'ts-node src/boot/index.ts'",
    "--test:assert": "tsc && ts-node test/index.ts",
    "--test:coverage": "nyc npm run test:assert",
    "--watch": "tsc --watch"
  },
  "types": "dist/index.d.ts",
  "version": "2021.05.02"
